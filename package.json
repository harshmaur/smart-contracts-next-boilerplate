{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-types": "typechain --target=truffle-v5 --out-dir ./src/types/truffle-contracts './src/abi/*.json'",
    "postinstall": "truffle compile && yarn generate-types",
    "migrate": "tsc --project ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "migrate:rinkeby": "tsc --project ./tsconfig.migrate.json --outDir ./migrations && truffle migrate --network rinkeby",
    "test:contracts": "truffle test ./src/test/*"
  },
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.9.0",
    "react-icons": "^4.2.0",
    "swr": "^0.5.6",
    "web3": "^1.3.6"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "@truffle/hdwallet-provider": "^1.4.1",
    "@typechain/truffle-v5": "^5.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.2.19",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.5",
    "@types/react": "17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "autoprefixer": "^10.2.6",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "mocha": "^9.0.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "tailwindcss": "^2.2.4",
    "truffle": "^5.3.13",
    "typechain": "^5.1.1",
    "typescript": "4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  }
}
